{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 20 version, safari >= 7, ie >= 11",
        "modules": false
      }
    ],
    "minify"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-transform-classes",
      {
        "loose": true
      }
    ],
    "array-includes",
    "babel-plugin-transform-async-to-promises"
  ],
  "comments": false
}